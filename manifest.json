{
  "manifest_version":2,
  "name" : "Go Anywhere",
  "version" : "1.0.0",
  "browser_action":{
   "default_popup":"popup.html"
  },
  "commands":{
    "open-youtube":{
      "suggested_key":{
        "default": "Ctrl+Y"
      },
      "description":"Open youTube"
    },
    "open-github":{
      "suggested_key":{
        "default": "Alt+G"
      },
      "description":"Open github"
    }
  },
  "background":{
    "scripts": ["background.js"],
    "persistent" : false
  },
  "permissions": [
    "tabs","activeTab"
  ]

}